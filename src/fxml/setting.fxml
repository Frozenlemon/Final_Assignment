<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<StackPane fx:id="settingPane" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="800.0" fitWidth="1100.0" pickOnBounds="true">
         <image>
            <Image url="@Images/background2.JPG" />
         </image>
      </ImageView>
      <VBox fx:id="vBoxMain" prefHeight="800.0" prefWidth="1100.0" stylesheets="@../fxml/style.css">
          <children>
              <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
                  <children>
                      <Pane prefHeight="70.0" prefWidth="831.0" HBox.hgrow="ALWAYS">
                          <children>
                              <Button id="backLabel" fx:id="btnBack" layoutX="28.0" layoutY="24.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="117.0" text="Back" />
                              <Button id="homeLabel" fx:id="btnHome" layoutX="164.0" layoutY="24.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="117.0" style="-fx-background-image: url('Images/cancel.png'); &quot;: ;" text="Home" />
                          </children>
                      </Pane>
                  </children>
                  <padding>
                      <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                  </padding>
              </HBox>
              <HBox prefHeight="500.0" prefWidth="1100.0">
                  <children>
                  <StackPane alignment="TOP_LEFT" prefHeight="500.0" prefWidth="1100.0">
                     <children>
                        <CheckBox id="en" fx:id="enCheckBox" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onMouseClicked="#clickOnCheckBox" selected="true" text="English" translateX="300.0" translateY="15.0" />
                        <CheckBox id="vn" fx:id="vnCheckBox" mnemonicParsing="false" onAction="#clickOnCheckBox" onMouseClicked="#clickOnCheckBox" text="Vietnamese" translateX="450.0" translateY="15.0" StackPane.alignment="TOP_LEFT" />
                              <Button fx:id="btnCancel" mnemonicParsing="false" prefHeight="50.0" prefWidth="100.0" text="Cancel" translateX="400.0" translateY="500.0" />
                              <Button fx:id="btnSave" mnemonicParsing="false" onAction="#saveSettng" prefHeight="50.0" prefWidth="100.0" text="Save" translateX="200.0" translateY="500.0" />
                              <Slider maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="50.0" prefWidth="900.0" showTickLabels="true" showTickMarks="true" translateX="150.0" translateY="400.0" value="50.0" />
                              <Label id="msLabel" fx:id="msVolume" prefHeight="50.0" prefWidth="100.0" text="Master" translateX="80.0" translateY="190.0">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Label>
                              <Label id="chooseLanguageLable" fx:id="chooseLanguageLabel" prefHeight="50.0" prefWidth="150.0" text="Choose Language" translateX="50.0">
                           <font>
                              <Font name="Consolas Bold" size="17.0" />
                           </font>
                        </Label>
                        <Slider maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="50.0" prefWidth="900.0" showTickLabels="true" showTickMarks="true" translateX="150.0" translateY="300.0" value="50.0" />
                        <Slider maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="50.0" prefWidth="900.0" showTickLabels="true" showTickMarks="true" snapToTicks="true" translateX="150.0" translateY="200.0" value="50.0" StackPane.alignment="TOP_LEFT" />
                        <Label id="efLabel" fx:id="efVolume" prefHeight="50.0" prefWidth="100.0" text="Effect" translateX="80.0" translateY="290.0">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Label>
                        <Label id="volumeControlLabel" fx:id="volumeControlLabel" prefHeight="50.0" prefWidth="150.0" text=" Volume Control" translateX="50.0" translateY="100.0">
                           <font>
                              <Font name="System Bold" size="15.0" />
                           </font>
                        </Label>
                        <Label id="bgmLabel" fx:id="bgmVolume" prefHeight="50.0" prefWidth="100.0" text="BGM" translateX="80.0" translateY="390.0">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Label>
                     </children>
                  </StackPane>
                  </children>
              </HBox>
          </children>
      </VBox>
   </children>
</StackPane>
