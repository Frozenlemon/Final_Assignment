<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<StackPane fx:id="playerSelectPane" alignment="TOP_LEFT" prefHeight="800.0" prefWidth="1010.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="808.0" fitWidth="1010.0" opacity="0.94" pickOnBounds="true">
         <image>
            <Image url="@Images/background2.JPG" />
         </image>
      </ImageView>
      <ImageView fitHeight="50.0" fitWidth="300.0" pickOnBounds="true" translateX="150.0" translateY="560.0">
         <image>
            <Image url="@Images/Blue.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="50.0" fitWidth="300.0" pickOnBounds="true" translateX="150.0" translateY="260.0">
         <image>
            <Image url="@Images/Green.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="50.0" fitWidth="300.0" pickOnBounds="true" translateX="150.0" translateY="410.0">
         <image>
            <Image url="@Images/Yellow.png" />
         </image>
      </ImageView>
      <TextField fx:id="txtRedName" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="50.0" prefWidth="400.0" translateX="510.0" translateY="110.0" />
      <TextField fx:id="txtGreenName" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="50.0" prefWidth="400.0" translateX="510.0" translateY="410.0" />
      <TextField fx:id="txtYellowName" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="50.0" prefWidth="400.0" translateX="510.0" translateY="260.0" />
      <TextField fx:id="txtBlueName" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="50.0" prefWidth="400.0" translateX="510.0" translateY="560.0" />
      <Button id="backLabel" fx:id="backButton" minHeight="60.0" mnemonicParsing="false" prefWidth="200.0" stylesheets="@ButtonMenu.css" text="Back" translateX="200.0" translateY="700.0">
         <font>
            <Font size="30.0" />
         </font></Button>
      <Label id="chooseLabel" fx:id="choosePlayer" prefHeight="34.0" prefWidth="420.0" text="Choose Color &amp; Enter Your Name" textFill="#0a0a0a" textOverrun="CLIP" translateX="300.0" translateY="30.0">
         <font>
            <Font name="Constantia Bold" size="26.0" />
         </font>
      </Label>
      <ImageView fitHeight="50.0" fitWidth="300.0" pickOnBounds="true" translateX="150.0" translateY="110.0">
         <image>
            <Image url="@Images/Red.png" />
         </image>
      </ImageView>
      <Button id="continueLabel" fx:id="continueButton" minHeight="60.0" mnemonicParsing="false" onAction="#continueGame" prefWidth="200.0" stylesheets="@ButtonMenu.css" text="Continue" translateX="620.0" translateY="700.0">
         <StackPane.margin>
            <Insets />
         </StackPane.margin>
         <font>
            <Font size="30.0" />
         </font></Button>
   </children>
</StackPane>
